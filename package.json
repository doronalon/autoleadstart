{
  "name": "autoleadtsart",
  "version": "0.0.1",
  "description": "",
  "author": "Doron Alon",
  "scripts": {
    "start": "node ./dist/sorted.paragraph.js",
    "build": "tsc --watch",
    "test": "tsc && jest"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "jest": "^29.3.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
